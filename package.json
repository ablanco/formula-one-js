{
	"name": "formula-one-js",
	"version": "1.0.11",
	"author": "christophe bellec",
	"homepage": "https://github.com/christophe77/formula-one-js",
	"engines": {
		"node": "12.x || 14.x || >= 16"
	},
	"keywords": [
		"formula",
		"formula 1",
		"formula one",
		"ergast",
		"ergast.com",
		"jolpica",
		"jolpi.ca",
		"api"
	],
	"main": "./dist/cjs/index.js",
	"module": "./dist/esm/index.js",
	"types": "./dist/esm/index.d.ts",
	"scripts": {
		"start": "yarn build && node ./dist/cjs/index.js",
		"build": "yarn build:esm && yarn build:cjs",
		"build:esm": "tsc",
		"build:cjs": "tsc --module commonjs --outDir dist/cjs",
		"prepublish": "yarn build",
		"lint": "eslint . --ext .ts",
		"test": "vitest run",
		"test:watch": "vitest"
	},
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^22.5.2",
		"eslint": "^9.9.1",
		"nodemon": "^3.1.4",
		"ts-node": "^10.9.2",
		"typescript": "^5.5.4",
		"vitest": "^3.0.8"
	},
	"dependencies": {
		"axios": "^1.7.7"
	}
}
